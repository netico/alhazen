<% include ../header.ejs %>

<div>
	<table  id="output" class="table table-striped table-bordered" style="width:100%"></table>
</div>
<script type="text/javascript" charset="utf-8">

jQuery.ajax({
	type: 'GET',
	dataType: 'json',
	url: '/get/local/json/straight-table/headers/' + sheet,
	success: function (columns) {
		$('#output').DataTable( {
			ajax: '/get/local/json/straight-table/data/' + sheet,
			columns: columns
		})
	},
	error: function (err) {
		console.log(err)
	}
});
</script>


